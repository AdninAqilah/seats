import 'package:cloud_firestore/cloud_firestore.dart';
import 'package:flutter/material.dart';

class AnnouncementsCustomer extends StatelessWidget {
  const AnnouncementsCustomer({Key? key}) : super(key: key);

  @override
  Widget build(BuildContext context) {
    return Container();
  }

  Stream<List<User>> readUsers() => FirebaseFirestore.instance.collection('restaurants')
      .snapshots()
  .map((snapshot) =>
  snapshot.docs.map((doc) => ).toList());
}
